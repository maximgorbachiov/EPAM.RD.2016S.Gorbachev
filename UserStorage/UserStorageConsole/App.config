<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  
  <configSections>
    <section name="Services" type="StorageConfigurator.ConfigSection.CustomSection, StorageConfigurator"/>
  </configSections>
  <Services>
      <ServiceInfo masterCount="1" slaveCount="4" />
  </Services>
    
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>

    <appSettings>
      <add key="fileName" value="userBase.xml" />
    </appSettings>
  
    <system.diagnostics>
        <trace autoflush="true"/>
        <sources>
          <source name="trace"
                  switchName="levelSwitch"
                  switchType="System.Diagnostics.SourceSwitch" >
            <listeners>            
              <add name="textwriterListener"
                type="System.Diagnostics.TextWriterTraceListener"
                initializeData="logInfo.txt"
              traceOutputOptions="ProcessId, DateTime"/>
              <add name="xmlListener"
                type="System.Diagnostics.XmlWriterTraceListener"
                initializeData="logInfo.xml"
                traceOutputOptions="DateTime" />
            </listeners>
          </source>
        </sources>
        <switches>
          <add name="boolSwitch" value="1"/>
          <add name="levelSwitch" value="Information" />
        </switches>
      </system.diagnostics>

</configuration>